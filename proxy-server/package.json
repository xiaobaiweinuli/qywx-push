{
  "name": "wechat-api-proxy",
  "version": "1.0.0",
  "description": "企业微信 API 代理服务器 - 解决 Cloudflare IP 白名单问题",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start server.js --name wechat-proxy",
    "pm2:stop": "pm2 stop wechat-proxy",
    "pm2:restart": "pm2 restart wechat-proxy",
    "pm2:logs": "pm2 logs wechat-proxy"
  },
  "keywords": [
    "wechat",
    "proxy",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
